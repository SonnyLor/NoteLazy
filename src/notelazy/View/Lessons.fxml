<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox id="HBox" prefHeight="530.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="notelazy.Ctrl.LessonsController">
 <children>
      <FlowPane prefHeight="400.0" prefWidth="106.0">
         <children>
            <Label prefHeight="35.0" prefWidth="93.0" text="%content.lessons">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
         </children>
      </FlowPane>
      <BorderPane prefHeight="530.0" prefWidth="636.0">
         <center>
            <TableView prefHeight="428.0" prefWidth="573.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn prefWidth="150.0" text="%content.lesson" />
                <TableColumn minWidth="0.0" prefWidth="304.0" text="%content.weight" />
                  <TableColumn prefWidth="75.0" text="%content.bloc" />
              </columns>
            </TableView>
         </center>
         <bottom>
            <Pane prefHeight="22.0" prefWidth="354.0" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="264.0" minWidth="10.0" prefWidth="264.0" />
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="154.0" minWidth="10.0" prefWidth="54.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <TextField fx:id="lesson" promptText="Lesson" GridPane.rowIndex="1"/>
                  <TextField fx:id="weight" accessibleHelp="Lesson" promptText="Weight" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <MenuButton fx:id="blocs" mnemonicParsing="false" prefHeight="25.0" prefWidth="179.0" text="Select bloc" GridPane.columnIndex="2" GridPane.rowIndex="1">
                    <items>
                      <MenuItem mnemonicParsing="false">
                           <graphic>
                              <TextField fx:id="newBloc" onAction="#addAndSelect" promptText="%content.new" />
                           </graphic></MenuItem>
                    </items>
                  </MenuButton>
                  <Button mnemonicParsing="false" onAction="#add" text="%content.add" GridPane.columnIndex="3" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </top>
      </BorderPane>
      <GridPane prefHeight="525.0" prefWidth="197.0">
        <columnConstraints>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="132.0" minWidth="10.0" prefWidth="132.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="66.0" minWidth="9.0" prefWidth="9.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#back" text="%content.back" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Button mnemonicParsing="false" onAction="#deleteSelected" text="%content.delete.selected" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</HBox>
